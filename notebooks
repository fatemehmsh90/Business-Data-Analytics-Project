      "cell_type": "code",
      "source": [
        "# Replace missing text fields with a simple placeholder\n",
        "text_columns = [\n",
        "    \"ProductIntroduction\", \"ProductUses\", \"ProductBenefits\",\n",
        "    \"SideEffect\", \"HowToUse\", \"HowWorks\", \"QuickTips\", \"SafetyAdvice\"\n",
        "]\n",
        "\n",
        "for col in text_columns:\n",
        "    mid_df[col] = mid_df[col].fillna(\"Not available\")"
      ],
      "metadata": {
        "id": "qS-0EgUzzYcD"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Clean and normalize text fields (lowercase + strip)\n",
        "def clean_text(x):\n",
        "    if isinstance(x, str):\n",
        "        return x.lower().strip()\n",
        "    return x\n",
        "\n",
        "for col in text_columns:\n",
        "    mid_df[col] = mid_df[col].apply(clean_text)"
      ],
      "metadata": {
        "id": "sMH-DfUyzgsE"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import re\n",
        "\n",
        "def remove_html(x):\n",
        "    if isinstance(x, str):\n",
        "        return re.sub(r\"<.*?>\", \"\", x)\n",
        "    return x\n",
        "\n",
        "for col in text_columns:\n",
        "    mid_df[col] = mid_df[col].apply(remove_html)"
      ],
      "metadata": {
        "id": "daPLpg5JznVU"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
